{
  "name": "@jason12306/vue-async-virtual-scroll",
  "version": "1.0.1",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build  --config vite.config.build.ts",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "pub": "npm run build-only && cd dist && npm publish --access public"
  },
  "main": "vue-async-virtual-scroll.js",
  "description": "High-performance asynchronous virtual scroll component for Vue 3 with dynamic height support and optimized rendering for large datasets",
  "keywords": [
    "vue3",
    "virtual-scroll",
    "async-virtual-scroll",
    "dynamic-height",
    "performance",
    "large-data",
    "vue-component",
    "scroll-optimization",
    "infinite-scroll",
    "data-rendering"
  ],
  "author": "Jason12306 <vanyi0924@gmail.com>",
  "homepage": "https://github.com/Jason12306/vue-async-virtual-scroll",
  "license": "MIT",
  "peerDependencies": {
    "vue": "3.5.x"
  },
  "dependencies": {
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.52.8",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.29.0",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "unplugin-dts": "1.0.0-beta.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10"
  }
}
